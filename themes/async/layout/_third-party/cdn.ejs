<% if(cdntype==='head') {%>

    <%- theme.cdn.css ? css(theme.cdn.css) : '' %>
    <%- theme.cdn.js.head ? jss(theme.cdn.js.head) : '' %>

<% } else if(cdntype==='body') { %>

    <%- theme.cdn.js.base ? js(theme.cdn.js.base) : '' %>

    <% if(theme.cdn.js.async) {%>
        <%- Array.isArray(theme.cdn.js.async)? js(theme.cdn.js.async.map(src=>({src, async: true}))): js({src:theme.cdn.js.async, async: true}) %>
    <% } %>

    <% if(theme.cdn.js.defer) {%>
        <%- Array.isArray(theme.cdn.js.defer)? js(theme.cdn.js.defer.map(src=>({src, defer: true}))): js({src:theme.cdn.js.defer, defer: true}) %>
    <% } %>

<% } %>